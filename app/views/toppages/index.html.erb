<div class="center jumbotron">
  <% if logged_in? %>
    <div class="center jumbotron">
      <div class="text-center">
        <h1>Dream scouts Member page</h1>
      </div>
    </div>
    <div>
      <%= link_to '連絡板', user_path(current_user) %> 
      <%= link_to 'メンバー情報', users_path %>
      <%= link_to '動画一覧' %>
      <%= link_to '楽譜一覧' %>
    </div>
  </div>
  <% else %>
    <div class="center jumbotron">
      <div class="text-center">
        <h1>Welcome to Dream scouts</h1>
      </div>
    </div>
  <% end %>
</div>